{
  "name": "nats",
  "version": "0.0.1",
  "type": "flogo:trigger",
  "ref": "github.com/sandstorm/flogo-nats/trigger/nats",
  "description": "listen to a NATS subject (or multiple of them)",
  "author": "Sebastian Kurfuerst <sebastian.kurfuerst@sandstorm.de>",
  "settings":[
    {
      "name": "setting",
      "type": "string",
      "value": "default"
    }
  ],
  "outputs": [
    {
      "name": "stream",
      "type": "string"
    },
    {
      "name": "content",
      "type": "any"
    }
  ],
  "handler": {
    "settings": [
      {
        "name": "handler_setting",
        "type": "string"
      }
    ]
  }
}